{
    "rules": {
        "indent": [ 1, 2 ],
        "quotes": [ 1, "double" ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ],
        "comma-dangle": [2, "always-multiline"],
        "no-console": [1],

    },
    "env": {
        "es6": true,
        "node": true,
        "browser" : true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
         "modules": true,
        "experimentalObjectRestSpread": true
    },
    "globals" :{
      "React": true,
      "jQuery": true,
    },
    "plugins": [
        "react"
    ]
}
